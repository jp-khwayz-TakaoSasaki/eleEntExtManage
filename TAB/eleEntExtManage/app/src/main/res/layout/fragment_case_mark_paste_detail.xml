        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_meisai_disp"
        app:layout_constraintStart_toEndOf="@id/tv_casemark_paste_lwh"
        app:layout_constraintTop_toTopOf="@id/tv_casemark_paste_lwh" />

    <TextView
