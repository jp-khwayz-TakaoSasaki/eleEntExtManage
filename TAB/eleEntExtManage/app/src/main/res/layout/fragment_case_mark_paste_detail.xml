        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_meisai_disp"
        app:layout_constraintStart_toStartOf="@id/guideline_01"
        app:layout_constraintTop_toBottomOf="@id/tv_casemark_paste_casemark_05" />

    <TextView
        android:id="@+id/tv_casemark_paste_casemark_07"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/casemark_paste_detail_casemark_hr"
        android:text="ケースマーク０７"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_meisai_disp"
        app:layout_constraintStart_toStartOf="@id/guideline_01"
