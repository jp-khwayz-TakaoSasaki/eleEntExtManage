    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_02"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier_hr_01"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="tv_casemark_paste_casemark_12,tv_casemark_paste_casemark_05,tv_casemark_paste_remarks"
        tools:layout_editor_absoluteY="200dp" />

    <!-- ヘッダ左側 -->
    <TextView
        android:id="@+id/tv_casemark_paste_casemark_01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="ELEMATEC"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_meisai_disp"
        app:layout_constraintStart_toStartOf="@id/guideline_01"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_casemark_paste_casemark_02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/casemark_paste_detail_casemark_hr"
        android:text="LAMPHUN"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_meisai_disp"
        app:layout_constraintStart_toStartOf="@id/guideline_01"
        app:layout_constraintTop_toBottomOf="@id/tv_casemark_paste_casemark_01" />

    <TextView
        android:id="@+id/tv_casemark_paste_casemark_03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/casemark_paste_detail_casemark_hr"
