        android:id="@+id/tv_casemark_paste_remarks"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/casemark_paste_detail_sprit_hr"
        android:text="@string/common_list_header_remarks_coron"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_meisai_disp"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@id/guideline_02"
        app:layout_constraintEnd_toStartOf="@id/tv_casemark_paste_remarks_value"
        app:layout_constraintTop_toBottomOf="@id/tv_casemark_paste_gw" />

    <TextView
        android:id="@+id/tv_casemark_paste_remarks_value"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="１２３４５６７８９０１２３４５６７８９０１２３４５６７８９０１２３４５６７８９０１２３４５６７８９０１２３４５６７８９０１２３４５６７８９０１２３４５６７８９０１２３４５６７８９０１２３４５６７８９０"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_meisai_disp"
        app:layout_constraintStart_toEndOf="@id/tv_casemark_paste_remarks"
        app:layout_constraintTop_toTopOf="@id/tv_casemark_paste_remarks"
        app:layout_constraintEnd_toEndOf="parent"/>

    <!-- 下段 -->
    <TextView
        android:id="@+id/tv_casemark_paste_invoice_no"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/casemark_paste_detail_sprit_hr"
        android:layout_marginEnd="20dp"
        android:text="INVOICENO"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_meisai_disp"
        app:layout_constraintEnd_toStartOf="@id/tv_casemark_paste_invoice_no_value"
