        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_meisai_disp"
        app:layout_constraintStart_toStartOf="@id/guideline_02"
        app:layout_constraintTop_toBottomOf="@id/tv_casemark_paste_packing_quantity" />

    <TextView
        android:id="@+id/tv_casemark_paste_c_slash_no_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1 - 100"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_meisai_disp"
        app:layout_constraintStart_toEndOf="@id/tv_casemark_paste_c_slash_no"
        app:layout_constraintTop_toTopOf="@id/tv_casemark_paste_c_slash_no" />

    <TextView
        android:id="@+id/tv_casemark_paste_lwh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/casemark_paste_detail_sprit_hr"
        android:text="@string/common_list_header_lwh_coron"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_meisai_disp"
        app:layout_constraintStart_toStartOf="@id/guideline_02"
        app:layout_constraintTop_toBottomOf="@id/tv_casemark_paste_c_slash_no" />

    <TextView
        android:id="@+id/tv_casemark_paste_lwh_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
