        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_meisai_disp"
        app:layout_constraintStart_toStartOf="@id/guideline_01"
        app:layout_constraintTop_toBottomOf="@id/tv_casemark_paste_casemark_02" />

    <TextView
